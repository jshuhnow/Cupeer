<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cupid">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_element_12"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_element_12"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.AboutActivity"></activity>
        <activity
            android:name=".view.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.QuizResultsActivity" />
        <activity android:name=".view.QuizQuestionsActivity" />
        <activity
            android:name=".view.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".view.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.cupid.view.MainActivity" />
        </activity>
        <activity android:name=".view.ChatActivity" />
        <activity android:name=".view.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>